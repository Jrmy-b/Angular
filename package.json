{
  "name": "mean2-app",
  "description": "Full-Stack JavaScript with MongoDB / PostgreSQL, Express, Angular, and Node.js.",
  "version": "0.0.0",
  "private": false,
  "authors": "https://github.com/weareopensource/meanjs-2/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weareopensource/meanjs-2"
  },
  "engines": {
    "node": ">=6.3.0",
    "npm": ">=3.10.8"
  },
  "angular-cli": {},
  "scripts": {
    "update": "npm update && npm prune",
    "clean": "rm -rf node_modules/",
    "reinstall": "npm cache clean && npm run clean && npm install",
    "start": "ng serve",
    "build:prod": "ng build --prod",
    "build:demo": "ng build --env=demo",
    "lint": "tslint \"client/**/*.ts\" --project client/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "stylelint": "stylelint 'client/**/*.scss'",
    "test": "ng test",
    "test:e2e": "protractor",
    "generate-ssl-certs": "scripts/generate-ssl-certs.sh"
  },
  "dependencies": {
    "@angular/animations": "^4.0.2",
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/material": "2.0.0-beta.5",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/platform-server": "^4.0.2",
    "@angular/router": "^4.0.2",
    "@types/redux-localstorage": "1.0.2",
    "angular-font-awesome": "^2.0.2",
    "async": "~2.0.1",
    "bower": "~1.7.7",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "express-hbs": "^1.0.2",
    "express-session": "~1.14.1",
    "express-swaggerize": "farhatmo/express-swaggerize",
    "file-stream-rotator": "~0.0.6",
    "font-awesome": "^4.7.0",
    "gulp-ava": "^0.15.0",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "jasmine-core": "~2.5.0",
    "mocha": "~3.1.0",
    "redux-localstorage": "1.0.0-rc5",
    "redux-logger": "^2.8.1",
    "request": "^2.79.0",
    "rxjs": "^5.1.0",
    "serve-favicon": "~2.3.0",
    "ts-helpers": "^1.1.1",
    "tslib": "^1.6.1",
    "typescript": "^2.2.2",
    "validator": "~6.0.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular-redux/router": "^6.0.0",
    "@angular-redux/store": "^6.2.0",
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "^4.0.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.38",
    "@types/node": "~6.0.60",
    "@types/redux-logger": "2.6.34",
    "async-to-gen": "^1.3.0",
    "ava": "^0.17.0",
    "codelyzer": "~2.0.0",
    "coveralls": "~2.11.6",
    "cz-conventional-changelog": "^1.2.0",
    "del": "^2.2.2",
    "eslint": "~2.2.0",
    "eslint-config-airbnb": "~6.0.2",
    "immutable": "^3.8.1",
    "istanbul": "~0.4.2",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~1.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.2.1",
    "lcov-result-merger": "~1.2.0",
    "mock-fs": "~3.11.0",
    "node-sass": "^4.5.0",
    "protractor": "~5.1.0",
    "raw-loader": "^0.5.1",
    "redux": "^3.4.0",
    "redux-observable": "^0.9.0",
    "run-sequence": "~1.2.2",
    "sass-loader": "^6.0.3",
    "should": "~11.1.0",
    "stylelint": "^7.10.1",
    "stylelint-config-primer": "^1.4.0",
    "stylelint-scss": "^1.4.4",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typed-immutable-record": "0.0.6",
    "typescript": "~2.2.0"
  },
  "ava": {
    "require": [
      "async-to-gen/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
